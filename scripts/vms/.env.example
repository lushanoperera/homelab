# API Keys for Media Stack Scripts
# Copy this file to .env and fill in your API keys
# Location on Flatcar VM: /srv/docker/media-stack/.env

# Overseerr API Key
# Get from: Overseerr Settings → General → API Key
OVERSEERR_API_KEY=your_overseerr_api_key_here

# Sonarr API Key
# Get from: Sonarr Settings → General → Security → API Key
SONARR_API_KEY=your_sonarr_api_key_here

# Radarr API Key
# Get from: Radarr Settings → General → Security → API Key
RADARR_API_KEY=your_radarr_api_key_here

# Service URLs (defaults work for standard docker-compose setup)
OVERSEERR_URL=http://localhost:5055
SONARR_URL=http://localhost:8989
RADARR_URL=http://localhost:7878
QBITTORRENT_URL=http://localhost:8080
